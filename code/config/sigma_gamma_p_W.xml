<?xml version="1.0"?>
<plot>
  <legend>
    <title>J/#psi at LHC, #sqrt{s}=7 TeV</title>
    <x1>0.5</x1>
    <y1>0.12</y1>
    <x2>0.87</x2>
    <y2>0.6</y2>
  </legend>
  <cms_energy>7e3</cms_energy>
  <meson_type>jpsi</meson_type>
  <filename>sigma_gamma_p_W.eps</filename>

  <x_axis>
    <title>W [GeV]</title>
    <low_range>3.1</low_range>
    <up_range>7e3</up_range>
    <log>true</log>
  </x_axis>
  <y_axis>
    <title>d#sigma(#gamma p #rightarrow Vp) [nb]</title>
    <low_range>1</low_range>
    <up_range>1000</up_range>
    <log>true</log>
  </y_axis>

  <plot>
    <sigma_gamma_p_model>powerlaw</sigma_gamma_p_model>
    <parameters>1.5 0.8 1</parameters>
    <legend_entry>#sigma_{#gamma p#rightarrow V p} #propto W^{0.8}</legend_entry>
    <process>gammap</process>
    <variable>w</variable>
    <line_style>1</line_style>
    <line_color>2</line_color>
  </plot>

  <plot>
    <sigma_gamma_p_model>reggeometry</sigma_gamma_p_model>
    <legend_entry>#sigma_{#gamma p#rightarrow V p} from eq.(2)</legend_entry>
    <process>gammap</process>
    <variable>w</variable>
    <line_style>3</line_style>
    <line_color>4</line_color>
  </plot>

  <plot>
    <sigma_gamma_p_model>logarithmic</sigma_gamma_p_model>
    <parameters>22 1</parameters>
    <legend_entry>#sigma_{#gamma p#rightarrow V p} = 10 #times log(W)</legend_entry>
    <process>gammap</process>
    <variable>w</variable>
    <line_style>4</line_style>
    <line_color>6</line_color>
  </plot>


  <plot>
    <sigma_gamma_p_model>powerlaw</sigma_gamma_p_model>
    <parameters>1.64179e+01 3.66651e-01 1</parameters>
    <legend_entry>#sigma_{#gamma p#rightarrow V p} = 16.4 #times W^{0.37}</legend_entry>
    <process>gammap</process>
    <variable>w</variable>
    <line_style>2</line_style>
    <line_color>3</line_color>
  </plot>


  <dataplot>
    <filename>../data/lhcb2014_sigwexp_wplus.txt</filename>
    <legend_entry>LHCb 930 pb^{-1} (W^{+} solutions)</legend_entry>
    <marker_style>20</marker_style>
    <marker_size>1</marker_size>
    <marker_color>1</marker_color>
  </dataplot>

  <dataplot>
    <filename>../data/lhcb2014_sigwexp_wminus.txt</filename>
    <legend_entry>LHCb 930 pb^{-1} (W^{-} solutions)</legend_entry>
    <marker_style>21</marker_style>
    <marker_size>1</marker_size>
    <marker_color>1</marker_color>
  </dataplot>

  <dataplot>
    <filename>../data/zeus_0201043_jpsi_ee.txt</filename>
    <legend_entry>ZEUS 38 pb^{-1}</legend_entry>
    <marker_style>20</marker_style>
    <marker_size>0.6</marker_size>
    <marker_color>4</marker_color>
  </dataplot>

  <dataplot>
    <filename>../data/zeus_0201043_jpsi_mumu.txt</filename>
    <legend_entry>ZEUS 55 pb^{-1}</legend_entry>
    <marker_style>20</marker_style>
    <marker_size>0.6</marker_size>
    <marker_color>6</marker_color>
  </dataplot>

  <dataplot>
    <filename>../data/h1_0510016_jpsi.txt</filename>
    <legend_entry>H1 55 pb^{-1}</legend_entry>
    <marker_style>20</marker_style>
    <marker_size>0.6</marker_size>
    <marker_color>7</marker_color>
  </dataplot>

  <dataplot>
    <filename>../data/h1_1304.5162_jpsi.txt</filename>
    <legend_entry>H1 140.8 pb^{-1}</legend_entry>
    <marker_style>20</marker_style>
    <marker_size>0.6</marker_size>
    <marker_color>8</marker_color>
  </dataplot>


</plot>

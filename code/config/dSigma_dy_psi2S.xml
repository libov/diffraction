<?xml version="1.0"?>
<plot>

  <legend>
    <title>#Psi(2S) at LHC, #sqrt{s}=7 TeV</title>
    <x1>0.375</x1>
    <y1>0.15</y1>
    <x2>0.775</x2>
    <y2>0.4</y2>
  </legend>

  <cms_energy>7e3</cms_energy>
  <meson_type>psi</meson_type>
  <filename>dSigma_dy_psi2S.eps</filename>
  <perform_fit>false</perform_fit>

  <x_axis>
    <title>Rapidity Y</title>
    <low_range>2</low_range>
    <up_range>4.5</up_range>
    <log>false</log>
  </x_axis>

  <y_axis>
    <title>d#sigma(pp #rightarrow pVp) / dY [nb]</title>
    <low_range>0</low_range>
    <up_range>1.2</up_range>
    <log>false</log>
  </y_axis>

  <plot>
    <sigma_gamma_p_model>powerlaw</sigma_gamma_p_model>
    <parameters>2.36408e+00 3.99748e-01 1</parameters>
    <legend_entry>#sigma_{#gamma p#rightarrow V p} = 2.36 #times W^{0.4}</legend_entry>
    <process>pp</process>
    <variable>y</variable>
    <line_style>1</line_style>
    <line_color>2</line_color>
    <use_in_the_fit>true</use_in_the_fit>
    <fix_parameters>0 0 1</fix_parameters>
  </plot>

  <dataplot>
    <filename>../data/psi2s_lhcb2014_dsdy.txt</filename>
    <legend_entry>LHCb 930 pb^{-1}</legend_entry>
    <marker_style>21</marker_style>
    <marker_size>1</marker_size>
    <marker_color>1</marker_color>
    <use_in_the_fit>true</use_in_the_fit>
  </dataplot>

</plot>

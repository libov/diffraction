<?xml version="1.0"?>
<plot>
  <legend>
    <title>J/#psi at LHC, #sqrt{s}=7 TeV</title>
    <x1>0.45</x1>
    <y1>0.25</y1>
    <x2>0.85</x2>
    <y2>0.5</y2>
  </legend>
  <cms_energy>7e3</cms_energy>
  <meson_type>jpsi</meson_type>
  <filename>dsigma_dW_nodata.eps</filename>

  <x_axis>
    <title>W [GeV]</title>
    <low_range>3.1</low_range>
    <up_range>7e3</up_range>
    <log>true</log>
  </x_axis>
  <y_axis>
    <title>d#sigma(#gamma p #rightarrow Vp) [nb]</title>
    <low_range>0</low_range>
    <up_range>0.4</up_range>
    <log>false</log>
  </y_axis>

  <plot>
    <sigma_gamma_p_model>powerlaw</sigma_gamma_p_model>
    <parameters>1.5 0.8 1</parameters>
    <legend_entry>#sigma_{#gamma p#rightarrow V p} #propto W^{0.8}</legend_entry>
    <process>pp</process>
    <variable>w</variable>
    <line_style>1</line_style>
    <line_color>2</line_color>
  </plot>

  <plot>
    <sigma_gamma_p_model>reggeometry</sigma_gamma_p_model>
    <legend_entry>#sigma_{#gamma p#rightarrow V p} from eq.(2)</legend_entry>
    <process>pp</process>
    <variable>w</variable>
    <line_style>3</line_style>
    <line_color>4</line_color>
  </plot>

</plot>
